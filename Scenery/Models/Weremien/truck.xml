<?xml version="1.0"?>
<PropertyList>

	<path>truck.ac</path>
	
	<offsets>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</offsets>

	<animation>
		<enable-hot type="bool">false</enable-hot>
	</animation>

  <animation>
    <type>scale</type>
    <x-offset>0.5</x-offset>
    <y-offset>0.5</y-offset>
    <z-offset>0.5</z-offset>
  </animation>
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5000</max-m>
	</animation>

</PropertyList>
