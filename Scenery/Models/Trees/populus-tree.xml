<?xml version="1.0"?>

<PropertyList>

 <path>populus-tree.ac</path>

 <animation>
  <type>billboard</type>
  <spherical>false</spherical>
 </animation>

  <animation>
    <type>scale</type>
    <x-offset>1.5</x-offset>
    <y-offset>1.5</y-offset>
    <z-offset>1.5</z-offset>
  </animation>
 
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5000</max-m>
 </animation>

</PropertyList>
